{
  "name": "protoqol/quark",
  "description": "A stand-alone - light-weight - no-nonsense - flat-file, database called Quark.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Quinten Justus",
      "email": "quinten@protoqol.xyz"
    }
  ],
  "minimum-stability": "beta",
  "require": {
    "symfony/console": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2"
  },
  "autoload-dev": {
    "psr-4": {
      "Protoqol\\Prequel\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  },
  "scripts-descriptions": {
    "test": "Runs the default PHPUnit test suite configured in phpunit.xml"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  }
}
